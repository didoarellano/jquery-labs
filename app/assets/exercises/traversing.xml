<?xml version="1.0" encoding="utf-8" ?>
<exercises>
  <exercise>
    <type>codemirror</type>
    <instructionsheading>Move $me to the beginning of the section</instructionsheading>
    <selector>section</selector>
    <iframehtml>
      <section>
        <article class="me">$me</article>
        <div>
          <article class="me">$me</article>
          <p>Paragraph</p>
        </div>
      </section>
      <article class="me">$me</article>
    </iframehtml>
  </exercise>
  <exercise>
    <type>codemirror</type>
    <instructionsheading>Select all h2s and ps that are children of a div</instructionsheading>
    <selector>div</selector>
    <iframehtml>
      <div>
        <h2> Heading2 1 </h2>
        <p> Paragraph 2</p>
        <h2> Heading2 2 </h2>
        <p> Paragraph 2</p>
      </div>
    </iframehtml>
  </exercise>
</exercises>
