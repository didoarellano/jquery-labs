<exercises>

  <exercise>
    <type>method</type>
    <instructionsheading>Make the <em>h3</em> go green</instructionsheading>
    <selector>li a</selector>
    <iframehtml>
      <ul class="gallery">

        <li class="t1 t5">
          <a href="#"> {Thumbnail} </a>
          <h3>Astronaut</h3>
          <p>Lorizzle ipsum dolor sit mofo, pot adipiscing yippiyo.</p>
        </li>

      </ul>
    </iframehtml>
  </exercise>

  <exercise>
    <type>method</type>
    <instructionsheading>Make all h2s and ps that are children of a div go green</instructionsheading>
    <selector>div</selector>
    <iframehtml>
      <div>
        <h2> Heading2 1 </h2>
        <p> Paragraph 2</p>
        <h2> Heading2 2 </h2>
        <p> Paragraph 2</p>
      </div>
    </iframehtml>
  </exercise>

  <exercise>
    <type>method</type>
    <instructionsheading>Make <em>ALL the divs</em> go green</instructionsheading>
