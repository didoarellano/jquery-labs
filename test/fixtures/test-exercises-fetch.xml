<?xml version="1.0" encoding="utf-8" ?>
<exercises>
    <exercise>
        <instructions>Move $me to the beginning of the section</instructions>
        <selector>section</selector>
        <iframehtml>
            <section>
                <article class="me">$me</article>
                <div>
                    <article class="me">$me</article>
                    <p>Paragraph</p>
                </div>
            </section>
            <article class="me">$me</article>
        </iframehtml>
    </exercise>
    <exercise>
        <instructions>Select all h2s and ps that are children of a div</instructions>
        <selector>div</selector>
        <iframehtml>
            <div>
                <h2> Heading2 1 </h2>
                <p> Paragraph 2</p>
                <h2> Heading2 2 </h2>
                <p> Paragraph 2</p>
            </div>
        </iframehtml>
    </exercise>
</exercises>
